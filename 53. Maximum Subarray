class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int cu=0; int Maxsum=INT_MIN;
        for(int i=0;i<nums.size();i++){
            cu +=nums[i];
            Maxsum=max(Maxsum,cu);
                if(cu<0){
                    cu=0;
                }
        }
        return Maxsum;
    }
};
