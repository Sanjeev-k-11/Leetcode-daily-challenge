class Solution {
public:
    int lengthOfLastWord(string s) {
        int count=0;
        bool st=false;
        for(int i=s.size()-1; i>=0; i--){
           if(s[i] !=' '){
            count++;
            st=true;
           }
           else if(st){
            break;
           }
        }
        return count;
    }
};
